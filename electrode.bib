	pages = {5025--5033},
	file = {Full Text PDF:/Users/nelson/Zotero/storage/XDGS9J8W/Berkeland et al. - 1998 - Minimization of ion micromotion in a Paul trap.pdf:application/pdf;Snapshot:/Users/nelson/Zotero/storage/2MGI6C5H/Minimization-of-ion-micromotion-in-a-Paul-trap.html:text/html},
}

