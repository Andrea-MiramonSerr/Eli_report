	author = {Ruster, T. and Schmiegelow, C. T. and Kaufmann, H. and Warschburger, C. and Schmidt-Kaler, F. and Poschinger, U. G.},
	month = sep,
	year = {2016},
	keywords = {Ambient Magnetic Field, Coherence Time, Permanent Magnet, Wait Time, Zeeman Splitting},
	pages = {254},
	file = {Full Text PDF:/Users/nelson/Zotero/storage/4XNR23QS/Ruster et al. - 2016 - A long-lived Zeeman trapped-ion qubit.pdf:application/pdf},
}

